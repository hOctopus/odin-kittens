<% provide(:title, "All the kittehs") %>

<%= link_to "Make a new fluff monster", new_kitten_path %>

<%= will_paginate %>

<% @kittens.each do |kitten| %>
  <ul>
    <%= link_to "Lookie #{kitten.name}!", kitten, method: :get %>
    <%= link_to "delete", kitten, method: :delete,
        data: { confirm: "Sure you want to delete this precious baby?" } %>
  </ul>
<% end %>

<%= will_paginate %>
